{
  "type": "extension",
  "name": "baidu_asr_python",
  "version": "0.1.0",
  "dependencies": [
    {
      "type": "system",
      "name": "ten_runtime_python",
      "version": "0.6"
    }
  ],
  "api": {
    "property": {
      "api_key": {
        "type": "string"
      },
      "app_id": {
        "type": "string"
      },
      "model": {
        "type": "string"
      },
      "sample_rate": {
        "type": "int64"
      }
    },
    "audio_frame_in": [
      {
        "name": "pcm_frame",
        "property": {}
      }
    ],
    "data_out": [
      {
        "name": "text_data",
        "property": {
          "end_of_segment": {
            "type": "bool"
          },
          "is_final": {
            "type": "bool"
          },
          "stream_id": {
            "type": "uint32"
          },
          "text": {
            "type": "string"
          }
        }
      }
    ]
  }
}