{
  "type": "extension",
  "name": "bytedance_asr_python",
  "version": "0.6.2",
  "dependencies": [
    {
      "type": "system",
      "name": "ten_runtime_python",
      "version": "0.6.2"
    }
  ],
  "package": {
    "include": [
      "manifest.json",
      "property.json",
      "BUILD.gn",
      "**.tent",
      "**.py",
      "README.md",
      "tests/**"
    ]
  },
  "api": {
    "property": {
      "api_key": {
        "type": "string"
      },
      "token": {
        "type": "string"
      },
      "cluster": {
        "type": "string"
      },
      "sample_rate": {
        "type": "int64"
      }
    },
    "audio_frame_in": [
      {
        "name": "pcm_frame",
        "property": {}
      }
    ],
    "cmd_in": [
      {
        "name": "on_user_joined",
        "property": {
          "user_id": {
            "type": "string"
          }
        }
      },
      {
        "name": "on_user_left",
        "property": {
          "user_id": {
            "type": "string"
          }
        }
      },
      {
        "name": "on_connection_failure",
        "property": {
          "error": {
            "type": "string"
          }
        }
      }
    ],
    "data_out": [
      {
        "name": "text_data",
        "property": {
          "time": {
            "type": "int64"
          },
          "duration_ms": {
            "type": "int64"
          },
          "language": {
            "type": "string"
          },
          "text": {
            "type": "string"
          },
          "is_final": {
            "type": "bool"
          },
          "stream_id": {
            "type": "uint32"
          },
          "end_of_segment": {
            "type": "bool"
          }
        }
      }
    ]
  }
}